@section scripts
{
    <script>
        $('#viewdetails')
            .on('shown.bs.collapse',
                function () {
                    $("#show-more-button").text("@Resource.View_Less »");
                });
        $('#viewdetails')
            .on('hidden.bs.collapse',
                function () {
                    $("#show-more-button").text("@Resource.View_More »");
                });

        var hasInternet = $('#@Html.IdFor(m => m.Step8FlatForSale.HasInternet)');
        var internetAreaDiv = "#internet-area-div";
        hasInternet.change(
            function () {
                handleInternet();
            });
        handleInternet();
        function handleInternet() {
            if ($(hasInternet).prop('checked')) {
                $(internetAreaDiv).show('slow');
                $(internetAreaDiv + " :input").prop("disabled", false);
            } else {
                $(internetAreaDiv).hide('slow');
                $(internetAreaDiv + " :input").prop("disabled", true);
            }

        }

        var hasRollerBlind = $('#@Html.IdFor(m => m.Step8FlatForSale.HasRollerBlind)');
        var rollerAreaDiv = "#roller-area-div";
        hasRollerBlind.change(
           function () {
               handleRollerBlind();
           });
        handleRollerBlind();

        function handleRollerBlind() {
            if ($(hasRollerBlind).prop('checked')) {
                $(rollerAreaDiv).show('slow');
                $(rollerAreaDiv + " :input").prop("disabled", false);
            } else {
                $(rollerAreaDiv).hide('slow');
                $(rollerAreaDiv + " :input").prop("disabled", true);
            }
        }

        var ShowBathroomLoader = function () {
            $('#addBathroomModal').modal('toggle');
            showLoader("#bathroom-area-data-div");
        }

        var HideBathroomLoader = function () {
            hideLoader("#bathroom-area-data-div");
        }
        SweetAjax("#bathroom-area-data-div", ".wt24-bathroom-confirmation");
    </script>
}