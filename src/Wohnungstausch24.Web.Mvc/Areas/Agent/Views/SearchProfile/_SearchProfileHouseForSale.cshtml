@using Wohnungstausch24.Core.EnumExtensions
@using Wohnungstausch24.Models.Enums
@using Wohnungstausch24.Resources
@model Wohnungstausch24.Models.ViewModels.Search.DetailedSearch.House.DetailedSearchHouseForSale



<li>
    @Resource.TypeOfMerchandising:
    <strong>
        @TypeOfMerchandising.Sale.GetDisplayName()
    </strong>
</li>
<li>
    @Resource.PropertyType:
    <strong>
        @PropertyType.House.GetDisplayName()
    </strong>
</li>

<li>
    @Resource.Ranged_PlotArea:
    <strong>
        @Html.DisplayFor(m=>m.PlotArea)
    </strong>
</li>
<li>
    @Resource.HouseType:
    <strong>
        @Model.HouseTypeViewModels.Select(c=>c.HouseType.GetDisplayName()).StringJoin(", ")
    </strong>
</li>