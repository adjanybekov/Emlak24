@using Wohnungstausch24.Web.Mvc.Helpers

<!DOCTYPE html>
<html lang="@CultureHelper.GetCurrentCulture()">
<head>
    <meta charset="UTF-8">
    <title>Realty Space - Real Estate Responsive HTML Theme</title>
    <!--[if IE]>
        <meta http-equiv="X-UA-Compatible" content="IE=9,chrome=1"><![endif]-->
